<ul class="tab-handles">
  <% is_first_tab = true %>
  <% tabs.each do |form_section| %>
      <% if is_first_tab %>
          <li class="current">
      <% else %>
          <li>
      <% end %>
      <a href="#tab_<%= form_section.section_name %>"><%= form_section.name %></a>
      </li>
      <% is_first_tab = false %>
  <% end %>
</ul>