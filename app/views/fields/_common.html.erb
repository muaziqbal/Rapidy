<p>
  <%= f.label :display_name %>
  <%= f.text_field :display_name %> <i>required</i>
</p>
<p>
  <%= f.label :help_text %>
  <%= f.text_field :help_text %>
</p>
<p>
  <%= f.label :enabled %>
  <%= f.check_box :enabled?, {}, true, false %>
</p>
<p>
	<% unless @field.new? %>
		<%= label_tag "destination_form_id", "Form" %>
  	<%= select_tag "destination_form_id", options_for_select(@forms_for_display, @form_section.unique_id) %>
	<% end %>
</p>
